{
  "stryker-config": {
    "reporters": [
      "progress",
      "markdown",
      "html"
    ],
    "mutate": [
      "!**/*Program.cs",
      "!**/Endpoints/*.cs",
      "!**/*ApplicationDbContext.cs",
      "!**/Migrations/*.cs"
    ],
    "ignore-methods": [
      "Log*"
    ],
    "test-case-filter": "Category!=E2E",
    "mutation-level": "Complete",
    "thresholds": {
      "high": 80,
      "low": 60,
      "break": 40
    }
  }
}